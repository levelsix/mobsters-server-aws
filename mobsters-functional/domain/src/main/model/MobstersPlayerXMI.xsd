  <xsd:simpleType name="MonsterType">
    <xsd:restriction base="xsd:NCName">
      <xsd:enumeration value="REGULAR"/>
      <xsd:enumeration value="MINI_BOSS"/>
      <xsd:enumeration value="BOSS"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ElementType">
    <xsd:restriction base="xsd:NCName">
      <xsd:enumeration value="NO_ELEMENT"/>
      <xsd:enumeration value="FIRE"/>
      <xsd:enumeration value="EARTH"/>
      <xsd:enumeration value="WATER"/>
      <xsd:enumeration value="ROCK"/>
      <xsd:enumeration value="LIGHT"/>
      <xsd:enumeration value="DARK"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType abstract="true" name="Director">
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
  </xsd:complexType>
  <xsd:element name="Director" type="player:Director"/>
  <xsd:complexType abstract="true" name="Player">
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
  </xsd:complexType>
  <xsd:element name="Player" type="player:Player"/>
  <xsd:complexType name="PlayerInternal">
    <xsd:complexContent>
      <xsd:extension base="player:Player">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="userData" type="player:UserDataInternal"/>
          <xsd:element name="ongoingTask" type="player:OngoingTaskInternal"/>
          <xsd:element name="completedTasks" type="player:CompletedTaskInternal"/>
          <xsd:element name="items" type="player:ItemInternal"/>
          <xsd:element name="monsters" type="player:MonsterInternal"/>
          <xsd:element name="teamSlots" type="player:MonsterInternal"/>
          <xsd:element name="pendingOperations" type="player:PendingOperationInternal"/>
        </xsd:choice>
        <xsd:attribute name="userUuid" type="xsd:string"/>
        <xsd:attribute name="gems" type="xsd:int"/>
        <xsd:attribute name="cash" type="xsd:int"/>
        <xsd:attribute name="oil" type="xsd:int"/>
        <xsd:attribute name="experience" type="xsd:int"/>
        <xsd:attribute name="teamSlots" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="PlayerInternal" type="player:PlayerInternal"/>
  <xsd:complexType abstract="true" name="UserData">
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
  </xsd:complexType>
  <xsd:element name="UserData" type="player:UserData"/>

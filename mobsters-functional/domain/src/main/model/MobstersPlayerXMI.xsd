<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:player="com.lvl6.mobsters.domainmodel.player" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="com.lvl6.mobsters.domainmodel.player">

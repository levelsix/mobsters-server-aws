        <xsd:attribute name="player" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="UserDataInternal" type="player:UserDataInternal"/>
  <xsd:complexType abstract="true" name="OngoingTask">
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
